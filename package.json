{
	"name": "nuxt-fsd-template",
	"type": "module",
	"version": "2.0.0",
	"private": false,
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "npx nuxi typecheck && npm run lint && nuxt build",
		"dev": "nuxt dev",
		"steiger": "npx steiger ./src",
		"steiger-watch": "npx steiger ./src -w",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "npm run steiger && eslint --fix",
		"pre-commit": "npm run steiger && lint-staged --allow-empty --config lint-staged.config.js",
		"prepare": "husky"
	},
	"dependencies": {
		"nuxt": "3.15.2",
		"vue": "3.5.13",
		"vue-router": "4.5.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "3.14.0",
		"@nuxt/eslint": "0.7.5",
		"eslint": "9.18.0",
		"husky": "9.1.7",
		"lint-staged": "15.3.0",
		"steiger": "0.5.5",
		"typescript": "5.7.3",
		"vue-tsc": "2.2.0"
	}
}
