{
	"name": "nuxt-app",
	"type": "module",
	"version": "1.1.0",
	"private": false,
	"engines": {
		"node": ">=18.13"
	},
	"scripts": {
		"build": "npx nuxi typecheck && npm run lint && nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "eslint --fix",
		"pre-commit": "lint-staged --allow-empty --config lint-staged.config.js",
		"prepare": "husky install"
	},
	"dependencies": {
		"nuxt": "3.10.3",
		"vue": "3.4.21",
		"vue-router": "4.3.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "2.8.3",
		"eslint": "8.57.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.2",
		"typescript": "5.3.3",
		"vue-tsc": "2.0.6"
	}
}
