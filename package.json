{
	"name": "nuxt-app",
	"type": "module",
	"version": "1.1.1",
	"private": false,
	"engines": {
		"node": ">=18.13"
	},
	"scripts": {
		"build": "npx nuxi typecheck && npm run lint && nuxt build",
		"dev": "nuxt dev",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "eslint --fix",
		"pre-commit": "lint-staged --allow-empty --config lint-staged.config.js",
		"prepare": "husky"
	},
	"dependencies": {
		"nuxt": "3.12.2",
		"vue": "3.4.31",
		"vue-router": "4.4.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "2.21.2",
		"@conarti/eslint-plugin-feature-sliced": "1.0.5",
		"eslint": "8.57.0",
		"husky": "9.0.11",
		"lint-staged": "15.2.7",
		"typescript": "5.5.2",
		"vue-tsc": "2.0.22"
	}
}
