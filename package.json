{
	"name": "nuxt-fsd-template",
	"type": "module",
	"version": "2.0.0",
	"private": false,
	"license": "MIT",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "npx nuxi typecheck && npm run lint && nuxt build",
		"dev": "nuxt dev",
		"steiger": "npx steiger ./src",
		"steiger-watch": "npx steiger ./src -w",
		"generate": "nuxt generate",
		"preview": "nuxt preview",
		"postinstall": "nuxt prepare",
		"lint": "npm run steiger && eslint --fix && stylelint . --fix",
		"pre-commit": "npm run steiger && lint-staged --allow-empty --config lint-staged.config.js",
		"prepare": "husky"
	},
	"dependencies": {
		"@nuxt/fonts": "0.10.3",
		"@nuxt/icon": "1.10.3",
		"nuxt": "3.15.4",
		"vue": "3.5.13",
		"vue-router": "4.5.0"
	},
	"devDependencies": {
		"@antfu/eslint-config": "4.8.1",
		"@nuxt/eslint": "1.1.0",
		"@pinia/nuxt": "0.10.1",
		"@stylistic/stylelint-config": "2.0.0",
		"eslint": "9.21.0",
		"husky": "9.1.7",
		"lint-staged": "15.4.3",
		"postcss": "8.5.3",
		"postcss-functions": "4.0.2",
		"postcss-html": "1.8.0",
		"postcss-pxtorem": "6.1.0",
		"postcss-sort-media-queries": "5.2.0",
		"sass-embedded": "1.85.1",
		"steiger": "0.5.5",
		"stylelint": "16.12.0",
		"stylelint-config-html": "1.1.0",
		"stylelint-config-recommended-scss": "14.1.0",
		"stylelint-config-recommended-vue": "1.6.0",
		"stylelint-order": "6.0.4",
		"sugarss": "5.0.0",
		"typescript": "5.5.3",
		"vue-tsc": "2.2.8"
	}
}
